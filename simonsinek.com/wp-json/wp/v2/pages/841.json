ll('.faq-heading');\n        const noResults =  document.querySelector('.no-results');\n        \n    \n        input.addEventListener(\"keyup\", function(event) {\n            let counter = 0;\n    \n            filter = input.value.toLowerCase();\n    \n            if (event.keyCode != 13) {\n                for (i = 0; i < nodes.length; i++) {\n                   \n                    if (nodes[i].innerText.toLowerCase().includes(filter) || nodesTitle[i].innerText.toLowerCase().includes(filter)) {\n                        nodes[i].parentNode.style.display = \"block\";\n                        nodes[i].style.display = \"none\";\n                        nodesTitle.forEach(function(element) {\n                            element.classList.remove(\"elementor-active\");\n                        });\n                        faqHeading.forEach(function(element) {\n                            element.style.display = \"none\";\n                        });\n                    } else {\n                        nodes[i].parentNode.style.display = \"none\";\n                    }\n                }\n            }\n    \n            if(filter == \"\"){\n                faqHeading.forEach(function(element) {\n                    element.style.display = \"block\";\n                });\n            }\n            nodesTab.forEach(function(element){\n                if(getComputedStyle(element).display == \"none\"){\n                    counter ++;\n                }\n                \n                if(nodesTab.length == counter){\n                    noResults.style.display = \"block\";\n                }else{\n                    noResults.style.display = \"none\";\n                }\n                \n                \n               \n            });\n        });\n        \n    \n    }, false);\n<\/script>\n\n<div class=\"inputHolder\">\n    <input type=\"text\" id=\"Search\" placeholder=\"Search for question...\" title=\"Type in a name\">\n    <svg width=\"24\" height=\"24